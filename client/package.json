{
  "name": "nest-typescript-starter-client",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "axios": "0.19.2",
    "vue": "2.7.16",
    "vue-router": "3.4.3",
    "vuex": "3.5.1",
    "vuex-map-fields": "1.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/runtime": "^7.24.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "autoprefixer": "9.4.5",
    "babel-loader": "8.0.5",
    "concurrently": "^5.3.0",
    "cross-env": "^5.2.0",
    "css-loader": "7.1.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "2.9.0",
    "null-loader": "^4.0.1",
    "postcss-loader": "3.0.0",
    "sass": "^1.77.1",
    "sass-lint": "1.12.1",
    "sass-loader": "^14.2.1",
    "vue-loader": "15.10.0",
    "vue-server-renderer": "^2.7.16",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "2.7.16",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
