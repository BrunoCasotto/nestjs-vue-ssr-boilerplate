{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/runtime": "7.4.3",
    "axios": "^0.19.2",
    "vue": "^2.7.16",
    "vue-router": "3.0.2",
    "vuex": "3.0.1",
    "vuex-map-fields": "1.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "autoprefixer": "9.4.5",
    "babel-loader": "8.0.5",
    "concurrently": "^5.3.0",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "3.0.0",
    "sass-lint": "1.12.1",
    "sass-loader": "7.1.0",
    "vue-loader": "15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^4.42.0",
    "webpack-cli": "3.3.0",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
